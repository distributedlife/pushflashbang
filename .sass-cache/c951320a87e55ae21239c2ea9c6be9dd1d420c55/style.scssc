div.jp-interface
{
  position: relative;
  background-color:#777;

  ul.jp-controls
  {
    li
    {
      position: absolute;
      display:inline;
    }

    a
    {
      position: absolute;
      overflow:hidden;
      text-indent:-9999px;
    }

    list-style-type:none;
    padding:0;
    margin: 0;
  }
}
a.jp-play
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/pause-icon.png") 0 0 no-repeat;
  display: none;
}
a.jp-play:hover
{
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause:hover
{
  background: url("/images/pause-icon.png") 0 0 no-repeat;
}
div.jp-progress
{
  position: absolute;
  overflow:hidden;
  background-color: #ddd;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
div.jp-seek-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;

  background-color: lightgrey;
  height:100%;
  cursor: pointer;
}
div.jp-play-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  background-color: blue;
  height:100%;
}

/* The seeking class is added/removed inside jPlayer */
div.jp-seeking-bg
{
  background: url("http://www.jplayer.org/latest/skin/pbar-ani.gif");
}


a.jp-mute
{
  width:22px;
  height:22px;
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-mute:hover
{
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-unmute
{
  width:222px;
