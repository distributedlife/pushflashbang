:@template"i<$table_row_base : #fff ;
$table_row_vary : #eee ;
$table_row_hover: #ffa ;

$page_background: #eee ;
$page_background_offset: #ddd ;
$li_background:   #fff ;
$pending_background: #ffd;
$success_notice_color: #efe;
$failure_notice_color: #fee;
$info_notice_color: #fff;
$help_notice_color: #faa;
$selected_item_color: #0f0;

$text_colour: black ;
$add_filter_colour: orange ;
$remove_filter_colour: red ;
$included_filter_colour: black ;
$excluded_filter_colour: grey;
$contrast_link_colour: orange;
$contrast_link_colour_faint: gray;

$button_positive: lightgreen ;
$button_positive_hover: darkgreen ;
$button_positive_outline: green ;
$button_negative: indianred;
$button_negative_hover: red;
$button_negative_outline: red;
$button_neutral: white;
$button_neutral_hover: black;
$button_neutral_outline: black;

$button_text: black;
$button_text_hover: white;

$font_size_letter: 3.0em;
$font_size_large: 1.6em;
$font_size_normal: 1.0em;
$font_size_smaller: 0.8em;
$font_size_small: 0.6em;
$font_size_tiny: 0.4em;

////////////////////////////////////////////////////////////////////////////////
// FONTS
////////////////////////////////////////////////////////////////////////////////

@font-face
{
  font-family: "AR PL KaitiM GB";
  src: local('AR PL UKai CN');
}

////////////////////////////////////////////////////////////////////////////////
// LAYOUT
////////////////////////////////////////////////////////////////////////////////

body
{
  width: 1000px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 0em;
  margin-bottom: 0em;
  font-size: $font_size_large ;
  background-color: $page_background;

  font-family: 'Times New Roman', serif;
}

li
{
  list-style: none;
}

.inline
{
  display: inline !important;
}

.inline-block
{
  display: inline-block !important;
}

header
{
  clear: both;
  margin-bottom: 0.6em ;
  margin-left: 0.6em ;
  margin-right: 0.6em ;

  padding: 0.6em;
}

.left
{
  float: left;
}

.right
{
  float: right;
}

.centre
{
  width: 100%;
  text-align: center ;
  margin-left: auto;
  margin-right: auto;
}

.tiny_pad_from_top
{
  padding-top: 0.6em;
}

.pad_from_top
{
  padding-top: 1.0em;
}

.panel
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin: 0.6em;
}

.panel-align
{
  padding: 0.6em;
  margin: 0.6em;
}

.top_menu_panel
{
  background-color: $page_background_offset;

  border-bottom-left-radius: 10px;
  -moz-border-radius-bottomleft: 10px;
  -webkit-border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  -moz-border-radius-bottomright: 10px;
  -webkit-border-bottom-right-radius: 10px;

  padding-left: 0.3em;
  padding-right: 0.3em;

  margin-left: 0.3em;
  margin-right: 0.3em;

  text-align: center;

    span
  {
    font-size: $font_size_small;
  }
}

.created_item
{
  background-color: $success_notice_color;
  border-radius: 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  padding: 0.6em;
  margin: 0.6em;
}

.search
{
  padding-right: 0.6em;

  float: right;
  clear: both;
}

.strong
{
  font-weight: bold;
}

legend.label
{
  font-weight: bold;
}

.section_break
{
  padding-top: 0.6em;
  border-bottom: 1px solid black;
}

.definition
{
  p
  {
    font-size: $font_size_small !important;
  }

  float: right;
  clear: none;
  margin-right: auto;
  margin-left: auto;
  text-align: center ;
}

.hidden
{
  display: none;
}

.nowrap
{
  white-space: nowrap;
}

em
{
  color: $contrast_link_colour;  
}

.section_left
{
  float: left;
}

.section_right
{
  float: right;
}

////////////////////////////////////////////////////////////////////////////////
// INTERACTIONS
////////////////////////////////////////////////////////////////////////////////

.pending
{
  background-color: $pending_background;
}

a
{
  text-decoration: none;
  color: $included_filter_colour;
}

a.contrast
{
  text-decoration: none;
  color: $contrast_link_colour;
}

a.contrast:visited
{
  text-decoration: none;
  color: $contrast_link_colour;
}

a:visited
{
  color: $included_filter_colour;
}

a:hover
{
  color: $add_filter_colour;
  text-decoration: underline;
}

textarea
{
  width: 90%;
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 2px solid $page_background;
  height: 6em;
  padding: 0.6em;
  display: block;
}

input
{
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 2px solid $page_background;
  padding: 0.6em;
  display: block;
}

input[type="text"]      { width: 90%; }
input[type="email"]     { width: 90%; }
input[type="password"]  { width: 90%; }
input[type="checkbox"]  { display: inline; }
input[type="radio"]     { display: inline; }

input[type="submit"]    {  display: inline; font-family: 'Times New Roman', serif; color: $button_text; }
input[type="submit"]:hover { color: $button_text_hover; }
input[type="submit"].positive { background: $button_positive; border: 1px solid $button_positive_outline; }
input[type="submit"].positive:hover { background: $button_positive_hover; }
input[type="submit"].negative { background: $button_negative; border: 1px solid $button_negative_outline; }
input[type="submit"].negative:hover { background: $button_negative_hover; }
input[type="submit"].neutral { background: $button_neutral; border: 1px solid $button_neutral_outline; }
input[type="submit"].neutral:hover { background: $button_neutral_hover; }

select { font-size: $font_size_small; border-radius: 5px; -moz-border-radius: 5px; -webkit-border-radius: 5px; display: block; }

.help { font-style: italics; color: $help_notice_color; }

form
{
  width: 100% !important;

  li
  {
    list-style: none;
    p { display: inline !important; padding: 0 !important; margin: 0 !important; }
  }

  li.width60 { width: 60%; }
  li.full_width { width: 100%; }

  a { margin: auto; font-size: $font_size_small; border-radius: 5px; -moz-border-radius: 5px; -webkit-border-radius: 5px; border: 2px solid $page_background; height: 6em; padding: 0.6em; color: $button_text; }
  a:hover { color: $button_text_hover; }
  a.positive { background: $button_positive; border: 1px solid $button_positive_outline; }
  a.positive:hover { background: $button_positive_hover; }
  a.negative { background: $button_negative; border: 1px solid $button_negative_outline; }
  a.negative:hover { background: $button_negative_hover; }
  a.neutral { background: $button_neutral; border: 1px solid $button_neutral_outline; }
  a.neutral:hover { background: $button_neutral_hover; }
}

.button
{
  a
  {
    margin: auto;
    font-size: $font_size_small;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border: 2px solid $page_background;
    padding-top: 0.6em;
    padding-bottom: 0.6em;
    padding-left: 1.2em;
    padding-right: 1.2em;
    color: $button_text;
  }

  a:hover { color: $button_text_hover; }
  a.positive { background: $button_positive; border: 1px solid $button_positive_outline; }
  a.positive:hover { background: $button_positive_hover; }
  a.negative { background: $button_negative; border: 1px solid $button_negative_outline; }
  a.negative:hover { background: $button_negative_hover; }
  a.neutral { background: $button_neutral; border: 1px solid $button_neutral_outline; }
  a.neutral:hover { background: $button_neutral_hover; }
}

.large_button { font-weight: bold; font-size: 1.2em; }
.big_button { font-weight: bold; font-size: 2em; }

nav
{
  span.include
  {
    a { color: $included_filter_colour; }
    a:visited { color: $included_filter_colour; }
    a:hover { color: $remove_filter_colour; }
  }

  span.exclude
  {
    a { color: $excluded_filter_colour; }
    a:visited { color: $excluded_filter_colour; }
    a:hover { color: $add_filter_colour; }
  }
}

.width35 { width: 35%; }

a.focus { color: $contrast_link_colour; }
a.focus:hover { color: $contrast_link_colour; font-weight: bold;}

a.large { font-size: 0.8em;}

////////////////////////////////////////////////////////////////////////////////
// STYLES
////////////////////////////////////////////////////////////////////////////////

.title { font-family: "Consolas", sans-serif; font-weight: bold; font-size: $font_size_large; }
.minor_title { clear: left; font-family: "Consolas", sans-serif; font-weight: bold; font-size: $font_size_normal; }
.mandarin { font-family: "AR PL KaitiM GB", sans-serif; }
.giant { font-weight: bold; font-size: 4em; text-align: center; }
.huge { font-weight: bold; font-size: 3em; text-align: center; }
.description { font-family: "Consolas", sans-serif; font-style: italic; font-size: $font_size_small; text-align: left; }
.subtitle { font-family: "Book Antiqua", serif;  font-style: italic; font-size: $font_size_small; }
.definition_text { font-family: "Book Antiqua", serif; font-style: italic; font-size: $font_size_normal; text-align: left; }

.menu   { font-size: $font_size_smaller; }
.subscript   { font-size: $font_size_small; }
.tiny_option { font-size: $font_size_tiny; }

.text_right { text-align: right; }
.text_left { text-align: left; }

.contrast { color: $contrast_link_colour; }
.contrast_faint { color: $contrast_link_colour_faint; }


////////////////////////////////////////////////////////////////////////////////
// CUSTOMISED
////////////////////////////////////////////////////////////////////////////////

.list_summary
{
  clear: both;
  margin-bottom: 0.6em;
  float: right;
  padding-left: 0.6em;
  padding-right: 0.6em;

  min-height: 1.0em;
}

.pad
{
  clear: both;
}

nav.filter
{
  clear: right;
  float: right;
  width: 30%;

  ul
  {
    margin-top: 0.6em;
    margin-bottom: 0.6em;

    li
    {
      font-size: $font_size_small ;
      padding: 0;
      margin: 0;
      text-align: right;
    } 
  }
}

.card_panel
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 100px;
  height: 75px;
}

.card_panel_full_page
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 155px;
  height: 100px;
}

////////////////////////////////////////////////////////////////////////////////
// Flash Notices
////////////////////////////////////////////////////////////////////////////////
.flash
{
  p
  {
    font-size: $font_size_small;
    padding: 0.6em;
    margin: 0.6em;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
  }
  
  p.success
  {
    background-color: $success_notice_color;
  }

  p.failure
  {
    background-color: $failure_notice_color;
  }
  
  p.error
  {
    background-color: $failure_notice_color;
  }

  p.notice
  {
    background-color: $info_notice_color;
  }

  p.info
  {
    background-color: $info_notice_color;
  }
}





#fade { /*--Transparent background layer--*/
	display: none; /*--hidden by default--*/
	background: #000;
	position: fixed; left: 0; top: 0;
	width: 100%; height: 100%;
	opacity: .80;
	z-index: 9999;
}
.popup_block{
	display: none; /*--hidden by default--*/
	background: #fff;
	padding: 20px;
	border: 20px solid #ddd;
	float: left;
	font-size: 1.2em;
	position: fixed;
	top: 50%; left: 50%;
	z-index: 99999;
	/*--CSS3 Box Shadows--*/
	-webkit-box-shadow: 0px 0px 20px #000;
	-moz-box-shadow: 0px 0px 20px #000;
	box-shadow: 0px 0px 20px #000;
	/*--CSS3 Rounded Corners--*/
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}
img.btn_close {
	float: right;
	margin: -55px -55px 0 0;
}

/*--Making IE6 Understand Fixed Positioning--*/
*html #fade {
	position: absolute;
}
*html .popup_block {
	position: absolute;
}




.jp-audio
{
  padding-top: 0.6em;


  .jp-type-play-only
  {
    padding-left: 0% !important;
    padding-right: 0% !important;

    .jp-interface
    {
      border-radius: 5px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      height:36px;
      border-bottom:none;
    }

    a.jp-play
    {
      top:7px;
      left:7px;
    }
    a.jp-pause
    {
      top:7px;
      left:7px;
    }
  }

  .jp-type-single
  {
    padding-left: 22%;
    padding-right: 22%;
    
    .jp-interface
    {
      border-radius: 5px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      height:36px;
      border-bottom:none;
    }
    
    a.jp-play
    {
      top:7px;
      left:7px;
    }
    a.jp-pause
    {
      top:7px;
      left:7px;
    }
    .jp-current-time
    {
      top:10px;
      left:42px;
    }
    .jp-progress {
      top: 9px;
      left:96px;
      width:100px;
      height:18px;
    }
    .jp-duration
    {
      top:10px;
      left:206px;
    }
    a.jp-mute
    {
      top:7px;
      left:256px;
    }
    a.jp-unmute {
      top:7px;
      left:256px;
    }
    .jp-volume-bar {
      top:14px;
      left:280px;
    }
  }
}
div.jp-interface
{
  position: relative;
  background-color:#777;

  ul.jp-controls
  {
    li
    {
      position: absolute;
      display:inline;
    }

    a
    {
      position: absolute;
      overflow:hidden;
      text-indent:-9999px;
    }

    list-style-type:none;
    padding:0;
    margin: 0;
  }
}
a.jp-play
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/pause-icon.png") 0 0 no-repeat;
  display: none;
}
a.jp-play:hover
{
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause:hover
{
  background: url("/images/pause-icon.png") 0 0 no-repeat;
}
div.jp-progress
{
  position: absolute;
  overflow:hidden;
  background-color: #ddd;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
div.jp-seek-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;

  background-color: lightgrey;
  height:100%;
  cursor: pointer;
}
div.jp-play-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  background-color: blue;
  height:100%;
}

/* The seeking class is added/removed inside jPlayer */
div.jp-seeking-bg
{
  background: url("http://www.jplayer.org/latest/skin/pbar-ani.gif");
}


a.jp-mute
{
  width:22px;
  height:22px;
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-mute:hover
{
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-unmute
{
  width:222px;
  height:22px;
  background: url("/images/volume-mute-icon.png") 0 0 no-repeat;
  display: none;
}
a.jp-unmute:hover
{
  background: url("/images/volume-mute-icon.png") 0 0 no-repeat;
}
div.jp-volume-bar
{
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  position: absolute;
  overflow:hidden;
  background-color: lightgrey;
  width:48px;
  height:8px;
  cursor: pointer;
}
div.jp-volume-bar-value
{
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  background-color: blue;
  width:0px;
  height:8px;
}



div.jp-current-time
{
  position: absolute;
  font-size:.64em;
  font-style:oblique;
}
div.jp-duration
{
  position: absolute;
  font-size:.64em;
  font-style:oblique;
  text-align: right;
