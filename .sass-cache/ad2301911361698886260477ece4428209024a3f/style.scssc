  clear: left;
  font-family: "Consolas", sans-serif;
  font-weight: bold;
  font-size: $font_size_normal;
}

p.description
{
  font-family: "Consolas", sans-serif;
  font-style: italic;
  font-size: $font_size_small;
  text-align: left;
}

p.subtitle
{
  font-family: "Book Antiqua", serif;
  font-style: italic;
  font-size: $font_size_small;
}

p.definition_text
{
  font-family: "Book Antiqua", serif;
  font-style: italic;
  font-size: $font_size_normal;
  text-align: left;
}

.tiny_option
{
  font-size: $font_size_tiny;
}

.subscript
{
  font-size: $font_size_small;
}

.text_right
{
  text-align: right;
}

.text_left
{
  text-align: left;
}

p.contrast
{
  color: $contrast_link_colour;
}


