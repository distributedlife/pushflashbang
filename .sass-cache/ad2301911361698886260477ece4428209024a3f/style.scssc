    width: 60%;
  }

  li.full_width
  {
    width: 100%;
  }

  a
  {
    margin: auto;
    font-size: $font_size_small;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border: 2px solid $page_background;
    height: 6em;
    padding: 0.6em;
    color: $button_text;
  }

  a:hover
  {
    color: $button_text_hover;
  }

  a.positive
