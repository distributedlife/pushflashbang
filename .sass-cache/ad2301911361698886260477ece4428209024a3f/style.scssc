// STYLES
////////////////////////////////////////////////////////////////////////////////

p.title
{
  font-family: "Consolas", sans-serif;
  font-weight: bold;
  font-size: $font_size_large;
}

p.mandarin
{
  font-family: "AR PL KaitiM GB", sans-serif;
}

p.giant
{
  font-weight: bold;
  font-size: 4em;
  text-align: center;
}

p.huge
{
  font-weight: bold;
  font-size: 3em;
  text-align: center;
}

p.minor_title
{
  clear: left;
  font-family: "Consolas", sans-serif;
  font-weight: bold;
  font-size: $font_size_normal;
}

p.description
{
  font-family: "Consolas", sans-serif;
  font-style: italic;
  font-size: $font_size_small;
  text-align: left;
}

p.subtitle
{
  font-family: "Book Antiqua", serif;
