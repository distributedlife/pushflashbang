  font-size: $font_size_small;
}

p.definition_text
{
  font-family: "Book Antiqua", serif;
  font-style: italic;
  font-size: $font_size_normal;
  text-align: left;
}

.tiny_option
{
  font-size: $font_size_tiny;
}

.subscript
{
  font-size: $font_size_small;
}

.text_right
{
  text-align: right;
}

.text_left
{
  text-align: left;
}

p.contrast
{
  color: $contrast_link_colour;
}


////////////////////////////////////////////////////////////////////////////////
// CUSTOMISED
////////////////////////////////////////////////////////////////////////////////

.list_summary
{
  clear: both;
  margin-bottom: 0.6em;
  float: right;
  padding-left: 0.6em;
  padding-right: 0.6em;

  min-height: 1.0em;
}

.pad
{
  clear: both;
}

nav.filter
{
  clear: right;
  float: right;
  width: 30%;

  ul
  {
    margin-top: 0.6em;
    margin-bottom: 0.6em;

    li
    {
      font-size: $font_size_small ;
      padding: 0;
      margin: 0;
      text-align: right;
    } 
  }
}

.card_panel
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 100px;
  height: 75px;
}

.card_panel_full_page
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 155px;
  height: 100px;
}

////////////////////////////////////////////////////////////////////////////////
// Flash Notices
////////////////////////////////////////////////////////////////////////////////
.flash
{
  p
  {
    font-size: $font_size_small;
    padding: 0.6em;
    margin: 0.6em;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
  }
  
  p.success
  {
    background-color: $success_notice_color;
  }

  p.failure
  {
    background-color: $failure_notice_color;
  }
  
  p.error
  {
    background-color: $failure_notice_color;
  }

  p.notice
  {
    background-color: $info_notice_color;
  }

  p.info
  {
    background-color: $info_notice_color;
  }
}





#fade { /*--Transparent background layer--*/
	display: none; /*--hidden by default--*/
	background: #000;
	position: fixed; left: 0; top: 0;
	width: 100%; height: 100%;
	opacity: .80;
	z-index: 9999;
}
.popup_block{
	display: none; /*--hidden by default--*/
	background: #fff;
	padding: 20px;
	border: 20px solid #ddd;
	float: left;
	font-size: 1.2em;
	position: fixed;
	top: 50%; left: 50%;
	z-index: 99999;
	/*--CSS3 Box Shadows--*/
	-webkit-box-shadow: 0px 0px 20px #000;
	-moz-box-shadow: 0px 0px 20px #000;
	box-shadow: 0px 0px 20px #000;
	/*--CSS3 Rounded Corners--*/
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}
img.btn_close {
	float: right;
	margin: -55px -55px 0 0;
}

/*--Making IE6 Understand Fixed Positioning--*/
*html #fade {
	position: absolute;
}
*html .popup_block {
	position: absolute;
}




