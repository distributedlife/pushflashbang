////////////////////////////////////////////////////////////////////////////////
// LAYOUT
////////////////////////////////////////////////////////////////////////////////

body
{
  width: 1000px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 0em;
  margin-bottom: 0em;
  font-size: $font_size_large ;
  background-color: $page_background;

  font-family: 'Times New Roman', serif;
}

li
{
  list-style: none;
}

header
{
  clear: both;
  margin-bottom: 0.6em ;
  margin-left: 0.6em ;
  margin-right: 0.6em ;

  padding-bottom: 0.6em;
  padding-left: 0.6em;
  padding-right: 0.6em;
}

.left
{
  float: left;
}

.right
{
  float: right;
}

.centre
{
  width: 100%;
  text-align: center ;
  margin-left: auto;
  margin-right: auto;
}

.pad_from_top
{
  padding-top: 1.0em;
}

.panel
{
  background-color: $li_background;
  border-radius: 20px;
  padding: 0.6em;
  margin: 0.6em;
}

.top_menu_panel
{
  background-color: $page_background_offset;

  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;

  padding-left: 0.3em;
  padding-right: 0.3em;

  margin-left: 0.3em;
  margin-right: 0.3em;

  text-align: center;

    span
  {
    font-size: $font_size_small;
  }
}

.created_item
{
  background-color: $success_notice_color;
  border-radius: 10px;
  padding: 0.6em;
  margin: 0.6em;
}

.search
{
  padding-right: 0.6em;

  float: right;
  clear: both;
}

.strong
{
  font-weight: bold;
}

legend.label
{
  font-weight: bold;
}

.section_break
{
  padding-top: 0.6em;
  border-bottom: 1px solid black;
}

.definition
{
  p
  {
    font-size: $font_size_small !important;
  }

  float: right;
  clear: none;
  margin-right: auto;
  margin-left: auto;
  text-align: center ;
}


////////////////////////////////////////////////////////////////////////////////
// INTERACTIONS
////////////////////////////////////////////////////////////////////////////////

.pending
{
  background-color: $pending_background;
}

a
{
  text-decoration: none;
  color: $included_filter_colour;
}

a:visited
{
  color: $included_filter_colour;
}

a:hover
{
  color: $add_filter_colour;
}

a:visited
{
}

a:hover
{
  text-decoration: none;
}

textarea
{
  width: 90%;
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  border: 2px solid $page_background;
  height: 6em;
  padding: 0.6em;
  display: block;
}

input
{
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  border: 2px solid $page_background;
  padding: 0.6em;
  display: block;
}

input[type="text"]
{
  width: 90%;
}

input[type="email"]
{
  width: 90%;
}

input[type="password"]
{
  width: 90%;
}

input[type="checkbox"]
{
  display: inline;
}

input[type="submit"]
{
  display: inline;
  font-family: 'Times New Roman', serif;
  color: $button_text;
}

input[type="submit"]:hover
{
  color: $button_text_hover;
}

input[type="submit"].positive
{
  background: $button_positive;
  border: 1px solid $button_positive_outline;
}

input[type="submit"].positive:hover
{
  background: $button_positive_hover;
}

input[type="submit"].negative
{
  background: $button_negative;
  border: 1px solid $button_negative_outline;
}

input[type="submit"].negative:hover
{
  background: $button_negative_hover;
}

input[type="submit"].neutral
{
  background: $button_neutral;
  border: 1px solid $button_neutral_outline;
}

input[type="submit"].neutral:hover
{
  background: $button_neutral_hover;
}

select
{
  font-size: $font_size_small;
  border-radius: 5px;
  display: block;
}

.help
{
  font-style: italics;
  color: $help_notice_color;
}

form
{
  width: 100% !important;

  li
  {
     list-style: none;

    p
    {
      display: inline !important;
      padding: 0 !important;
      margin: 0 !important;
    }
  }

  li.width60
  {
    width: 60%;
  }

  li.full_width
  {
    width: 100%;
  }

  a
  {
    margin: auto;
    font-size: $font_size_small;
    border-radius: 5px;
    border: 2px solid $page_background;
    height: 6em;
    padding: 0.6em;
    color: $button_text;
  }

  a:hover
  {
    color: $button_text_hover;
  }

  a.positive
  {
    background: $button_positive;
    border: 1px solid $button_positive_outline;
  }

  a.positive:hover
  {
    background: $button_positive_hover;
  }

  a.negative
  {
    background: $button_negative;
    border: 1px solid $button_negative_outline;
  }

  a.negative:hover
  {
    background: $button_negative_hover;
  }

  a.neutral
  {
    background: $button_neutral;
    border: 1px solid $button_neutral_outline;
  }

  a.neutral:hover
  {
    background: $button_neutral_hover;
  }
}

.button
{
  a
  {
    margin: auto;
    font-size: $font_size_small;
    border-radius: 5px;
    border: 2px solid $page_background;
    height: 6em;
    padding-top: 0.6em;
    padding-bottom: 0.6em;
    padding-left: 1.2em;
    padding-right: 1.2em;
    color: $button_text;
  }

  a:hover
  {
    color: $button_text_hover;
  }

  a.positive
  {
    background: $button_positive;
    border: 1px solid $button_positive_outline;
  }

  a.positive:hover
  {
    background: $button_positive_hover;
  }

  a.negative
  {
    background: $button_negative;
    border: 1px solid $button_negative_outline;
  }

  a.negative:hover
  {
    background: $button_negative_hover;
  }

  a.neutral
  {
    background: $button_neutral;
    border: 1px solid $button_neutral_outline;
  }

  a.neutral:hover
  {
    background: $button_neutral_hover;
  }
}

.large_button
{
  font-weight: bold;
  font-size: 1.2em;
}

.big_button
{
  font-weight: bold;
  font-size: 2em;
}

nav
{
  span.include
  {
    a
    {
      color: $included_filter_colour;
    }

    a:visited
    {
      color: $included_filter_colour;
    }

    a:hover
    {
      color: $remove_filter_colour;
    }
  }

  span.exclude
  {
    a
    {
      color: $excluded_filter_colour;
    }

    a:visited
    {
      color: $excluded_filter_colour;
    }

    a:hover
    {
      color: $add_filter_colour;
    }
  }
}

.width35
{
  width: 35%;
}

////////////////////////////////////////////////////////////////////////////////
// STYLES
////////////////////////////////////////////////////////////////////////////////

p.title
{
  font-family: "Consolas", sans-serif;
  font-weight: bold;
  font-size: $font_size_large;
}

p.giant
{
/*  font-family: "AR PL KaitiM GB";*/
  font-family: "Èö∂‰π¶";
  font-weight: bold;
  font-size: 4em;
  text-align: center;
}

p.minor_title
{
  font-family: "Consolas", sans-serif;
  font-weight: bold;
  font-size: $font_size_normal;
}

p.description
{
  font-family: "Consolas", sans-serif;
  font-style: italic;
  font-size: $font_size_small;
  text-align: left;
}

p.subtitle
{
  font-family: "Book Antiqua", serif;
  font-style: italic;
  font-size: $font_size_small;
}

p.definition_text
{
  font-family: "Book Antiqua", serif;
  font-style: italic;
  font-size: $font_size_normal;
  text-align: left;
}

.subscript
{
  font-size: $font_size_small;
}

////////////////////////////////////////////////////////////////////////////////
// CUSTOMISED
////////////////////////////////////////////////////////////////////////////////

.list_summary
{
  clear: both;
  margin-bottom: 0.6em;
  float: right;
  padding-left: 0.6em;
  padding-right: 0.6em;

  min-height: 1.0em;
}

.pad
{
  clear: both;
}

nav.filter
{
  clear: right;
  float: right;
  width: 30%;

  ul
  {
    margin-top: 0.6em;
    margin-bottom: 0.6em;

    li
    {
      font-size: $font_size_small ;
      padding: 0;
      margin: 0;
      text-align: right;
    } 
  }
}

////////////////////////////////////////////////////////////////////////////////
// Flash Notices
////////////////////////////////////////////////////////////////////////////////
.flash
{
  p
  {
    font-size: $font_size_small;
    padding: 0.6em;
    margin: 0.6em;
    border-radius: 5px;
  }
  
  p.success
  {
    background-color: $success_notice_color;
  }

  p.failure
  {
    background-color: $failure_notice_color;
  }
  
  p.error
  {
    background-color: $failure_notice_color;
  }

  p.notice
  {
    background-color: $info_notice_color;
  }

  p.info
  {
    background-color: $info_notice_color;
  }
}

