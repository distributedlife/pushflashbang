// CUSTOMISED
////////////////////////////////////////////////////////////////////////////////

.list_summary
{
  clear: both;
  margin-bottom: 0.6em;
  float: right;
  padding-left: 0.6em;
  padding-right: 0.6em;

  min-height: 1.0em;
}

.pad
{
  clear: both;
}

nav.filter
{
  clear: right;
  float: right;
  width: 30%;

  ul
  {
    margin-top: 0.6em;
    margin-bottom: 0.6em;

    li
    {
      font-size: $font_size_small ;
      padding: 0;
      margin: 0;
      text-align: right;
    } 
  }
}

.card_panel
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 100px;
  height: 75px;
}

.card_panel_full_page
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin-top: 0.6em;
  margin-left: 0.3em;
  margin-right: 0.3em;
  margin-bottom: 0.3em;
  width: 155px;
  height: 100px;
}

////////////////////////////////////////////////////////////////////////////////
// Flash Notices
////////////////////////////////////////////////////////////////////////////////
.flash
{
  p
  {
    font-size: $font_size_small;
    padding: 0.6em;
    margin: 0.6em;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
  }
  
  p.success
  {
    background-color: $success_notice_color;
  }

  p.failure
  {
    background-color: $failure_notice_color;
  }
  
  p.error
  {
    background-color: $failure_notice_color;
  }

  p.notice
  {
    background-color: $info_notice_color;
  }

  p.info
  {
    background-color: $info_notice_color;
  }
}





#fade { /*--Transparent background layer--*/
	display: none; /*--hidden by default--*/
	background: #000;
	position: fixed; left: 0; top: 0;
	width: 100%; height: 100%;
	opacity: .80;
	z-index: 9999;
}
.popup_block{
	display: none; /*--hidden by default--*/
	background: #fff;
	padding: 20px;
	border: 20px solid #ddd;
	float: left;
	font-size: 1.2em;
	position: fixed;
	top: 50%; left: 50%;
	z-index: 99999;
	/*--CSS3 Box Shadows--*/
	-webkit-box-shadow: 0px 0px 20px #000;
	-moz-box-shadow: 0px 0px 20px #000;
	box-shadow: 0px 0px 20px #000;
	/*--CSS3 Rounded Corners--*/
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}
img.btn_close {
	float: right;
	margin: -55px -55px 0 0;
}

/*--Making IE6 Understand Fixed Positioning--*/
*html #fade {
	position: absolute;
}
*html .popup_block {
	position: absolute;
}




.jp-audio
{
  padding-top: 0.6em;
  padding-left: 22%;
  padding-right: 22%;

  .jp-type-single
  {
    .jp-interface
    {
      border-radius: 5px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      height:36px;
      border-bottom:none;
    }
    
    a.jp-play
    {
      top:7px;
      left:7px;
    }
    a.jp-pause
    {
      top:7px;
      left:7px;
    }
    .jp-current-time
    {
      top:10px;
      left:42px;
    }
    .jp-progress {
      top: 9px;
      left:96px;
      width:100px;
      height:18px;
    }
    .jp-duration
    {
      top:10px;
      left:206px;
    }
    a.jp-mute
    {
      top:7px;
      left:256px;
    }
    a.jp-unmute {
      top:7px;
      left:256px;
    }
    .jp-volume-bar {
      top:14px;
      left:280px;
    }
  }
}
div.jp-interface
{
  position: relative;
  background-color:#777;

  ul.jp-controls
  {
    li
    {
      position: absolute;
      display:inline;
    }

    a
    {
      position: absolute;
      overflow:hidden;
      text-indent:-9999px;
    }

    list-style-type:none;
    padding:0;
    margin: 0;
  }
}
a.jp-play
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause
{
  width:22px;
  height:22px;
  z-index:1;
  background: url("/images/pause-icon.png") 0 0 no-repeat;
  display: none;
}
a.jp-play:hover
{
  background: url("/images/play-icon.png") 0 0 no-repeat;
}
a.jp-pause:hover
{
  background: url("/images/pause-icon.png") 0 0 no-repeat;
}
div.jp-progress
{
  position: absolute;
  overflow:hidden;
  background-color: #ddd;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
div.jp-seek-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;

  background-color: lightgrey;
  height:100%;
  cursor: pointer;
}
div.jp-play-bar
{
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  background-color: blue;
  height:100%;
}

/* The seeking class is added/removed inside jPlayer */
div.jp-seeking-bg
{
  background: url("http://www.jplayer.org/latest/skin/pbar-ani.gif");
}


a.jp-mute
{
  width:22px;
  height:22px;
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-mute:hover
{
  background: url("/images/volume-full-icon.png") 0 0 no-repeat;
}
a.jp-unmute
{
  width:222px;
  height:22px;
  background: url("/images/volume-mute-icon.png") 0 0 no-repeat;
  display: none;
}
a.jp-unmute:hover
{
  background: url("/images/volume-mute-icon.png") 0 0 no-repeat;
}
div.jp-volume-bar
{
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  position: absolute;
  overflow:hidden;
  background-color: lightgrey;
  width:48px;
  height:8px;
  cursor: pointer;
}
div.jp-volume-bar-value
{
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  background-color: blue;
  width:0px;
  height:8px;
}



div.jp-current-time
{
  position: absolute;
  font-size:.64em;
  font-style:oblique;
}
div.jp-duration
{
  position: absolute;
  font-size:.64em;
  font-style:oblique;
  text-align: right;
