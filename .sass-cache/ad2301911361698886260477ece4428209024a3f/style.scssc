////////////////////////////////////////////////////////////////////////////////
// FONTS
////////////////////////////////////////////////////////////////////////////////

@font-face
{
  font-family: "AR PL KaitiM GB";
  src: local('AR PL UKai CN');
}

////////////////////////////////////////////////////////////////////////////////
// LAYOUT
////////////////////////////////////////////////////////////////////////////////

body
{
  width: 1000px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 0em;
  margin-bottom: 0em;
  font-size: $font_size_large ;
  background-color: $page_background;

  font-family: 'Times New Roman', serif;
}

li
{
  list-style: none;
}

.inline
{
  display: inline !important;
}

.inline-block
{
  display: inline-block !important;
}

header
{
  clear: both;
  margin-bottom: 0.6em ;
  margin-left: 0.6em ;
  margin-right: 0.6em ;

  padding: 0.6em;
}

.left
{
  float: left;
}

.right
{
  float: right;
}

.centre
{
  width: 100%;
  text-align: center ;
  margin-left: auto;
  margin-right: auto;
}

.tiny_pad_from_top
{
  padding-top: 0.6em;
}

.pad_from_top
{
  padding-top: 1.0em;
}

.panel
{
  background-color: $li_background;
  border-radius: 20px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  padding: 0.6em;
  margin: 0.6em;
}

.top_menu_panel
{
  background-color: $page_background_offset;

  border-bottom-left-radius: 10px;
  -moz-border-radius-bottomleft: 10px;
  -webkit-border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  -moz-border-radius-bottomright: 10px;
  -webkit-border-bottom-right-radius: 10px;

  padding-left: 0.3em;
  padding-right: 0.3em;

  margin-left: 0.3em;
  margin-right: 0.3em;

  text-align: center;

    span
  {
    font-size: $font_size_small;
  }
}

.created_item
{
  background-color: $success_notice_color;
  border-radius: 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  padding: 0.6em;
  margin: 0.6em;
}

.search
{
  padding-right: 0.6em;

  float: right;
  clear: both;
}

.strong
{
  font-weight: bold;
}

legend.label
{
  font-weight: bold;
}

.section_break
{
  padding-top: 0.6em;
  border-bottom: 1px solid black;
}

.definition
{
  p
  {
    font-size: $font_size_small !important;
  }

  float: right;
  clear: none;
  margin-right: auto;
  margin-left: auto;
  text-align: center ;
}

.hidden
{
  display: none;
}

.nowrap
{
  white-space: nowrap;
}

em
{
  color: $contrast_link_colour;  
}

////////////////////////////////////////////////////////////////////////////////
// INTERACTIONS
////////////////////////////////////////////////////////////////////////////////

.pending
{
  background-color: $pending_background;
}

a
{
  text-decoration: none;
  color: $included_filter_colour;
}

a.contrast
{
  text-decoration: none;
  color: $contrast_link_colour;
}

a.contrast:visited
{
  text-decoration: none;
  color: $contrast_link_colour;
}

a:visited
{
  color: $included_filter_colour;
}

a:hover
{
  color: $add_filter_colour;
}

a:visited
{
}

a:hover
{
  text-decoration: none;
}

textarea
{
  width: 90%;
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 2px solid $page_background;
  height: 6em;
  padding: 0.6em;
  display: block;
}

input
{
  margin: auto;
  font-size: $font_size_small;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 2px solid $page_background;
  padding: 0.6em;
  display: block;
}

input[type="text"]
{
  width: 90%;
}

input[type="email"]
{
  width: 90%;
}

input[type="password"]
{
  width: 90%;
}

input[type="checkbox"]
{
  display: inline;
}

input[type="radio"]
{
  display: inline;
}

input[type="submit"]
{
  display: inline;
  font-family: 'Times New Roman', serif;
  color: $button_text;
}

input[type="submit"]:hover
{
  color: $button_text_hover;
}

input[type="submit"].positive
{
  background: $button_positive;
  border: 1px solid $button_positive_outline;
}

input[type="submit"].positive:hover
{
  background: $button_positive_hover;
}

input[type="submit"].negative
{
  background: $button_negative;
  border: 1px solid $button_negative_outline;
}

input[type="submit"].negative:hover
{
  background: $button_negative_hover;
}

input[type="submit"].neutral
{
  background: $button_neutral;
  border: 1px solid $button_neutral_outline;
}

input[type="submit"].neutral:hover
{
  background: $button_neutral_hover;
}

select
{
  font-size: $font_size_small;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  display: block;
}

.help
{
  font-style: italics;
  color: $help_notice_color;
}

form
{
  width: 100% !important;

  li
  {
     list-style: none;

    p
    {
      display: inline !important;
      padding: 0 !important;
      margin: 0 !important;
    }
  }

  li.width60
